<script src="https://code.jquery.com/jquery-3.3.1.js" ntegrity="sha256-2Kok7MbOyxpgUVvAk/HJ2jigOSYS2auK4Pfzbm7uH60=" crossorigin="anonymous"></script>
<script type="text/javascript">
$('#car_type').change(function(){
  var carId = $(this).val();
  if (carId == "") {
    alert("Please select car first");
    $('select[name="car-model"]').html("");
    return false;
  }
  $.ajax({
      type: "GET",
      url: "/home/get_car_models",
      data: {car_id : carId },
      success: function (response) {
        var option = '';
        for (var i=0;i<response.length;i++){
           option += '<option value="'+ response[i] + '">' + response[i] + '</option>';
        }
        $('select[name="car-model"]').html(option);
      }
  });
});
</script>
<script type="text/javascript">
$(document).ready(function() {
  $('#form1').submit(function() {
    var carId = $("#car_type").val();
    var carModel = $("#carModel").val();
    $(this).attr('action', 'car_repair/'+carId+'/'+carModel); 
    // return false;
  });
});
</script>
